<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChatController">
    <children>
        <Pane maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" nodeOrientation="LEFT_TO_RIGHT" opacity="0.89" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Button fx:id="sendButton" layoutX="513.0" layoutY="373.0" mnemonicParsing="false" text="Отправить" textAlignment="CENTER" textOverrun="CLIP" onAction="#sendMessage">
                    <font>
                        <Font name="System Italic" size="13.0" />
                    </font>
                </Button>
                <TextField fx:id="message" alignment="TOP_LEFT" layoutY="373.0" prefHeight="27.0" prefWidth="513.0" onAction="#onEnter"/>
                <ListView fx:id="messageList" prefHeight="373.0" prefWidth="600.0" />
            </children>
            <cursor>
                <Cursor fx:constant="DEFAULT" />
            </cursor>
            <effect>
                <Blend />
            </effect>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </Pane>
    </children>
</AnchorPane>
